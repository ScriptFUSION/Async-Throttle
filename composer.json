{
  "name": "async/throttle",
  "description": "Throttles async object throughput.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "require": {
    "php": "^7.2|^8",
    "amphp/amp": "^2.2"
  },
  "require-dev": {
    "amphp/phpunit-util": "^1.1",
    "infection/infection": ">=0.13,<1",
    "phpunit/phpunit": "^8.5|^9"
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Async\\Throttle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Async\\Throttle\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit -c test"
  },
  "config": {
    "sort-packages": true
  }
}
